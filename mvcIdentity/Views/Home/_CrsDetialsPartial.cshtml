@model mvcIdentity.Models.ApplyForCourse
       <div class="row">

            <div class="col-md-12">
                  <div class="header">
                        <h4 class="title">@Html.DisplayFor(model => model.course.Crs_Name)</h4>
                        
                        <div>
                            @{
                                if (ViewBag.result != null)
                                {
                                    <h3>@ViewBag.erorr</h3>
                                }
                            }
                        </div>

                        <div class="content">

                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        @Html.DisplayFor(model => model.course.Crs_Name)
                                        <div class="col-md-4">
                                            Name:
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group">

                                        @Html.DisplayFor(model => model.course.Crs_Description)
                                        <div class="col-md-4">
                                         Desc:
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        @Html.DisplayFor(model => model.course.Crs_Duration)

                                        <div class="col-md-4">
                                            Duration:
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        @Html.DisplayFor(model => model.course.Crs_Price)
                                        <div class="col-md-4">
                                            Price:
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group">

                                        @Html.DisplayFor(model => model.course.Topic.Top_Name)
                                        <div class="col-md-4">
                                            Topic:
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group">

                                        @Html.DisplayFor(model => model.ApplyDate)
                                        <div class="col-md-4">
                                            Date:

                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="clearfix"></div>

                        </div>
                    </div>
            </div>

        </div>


